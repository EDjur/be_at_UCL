<h1>Listing users</h1>
<p></p>

<% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <br>
      <td><%= user.email %></td>
      <br>
      <td><%= link_to 'Show', user_path(user) %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Destroy', user_path(user), method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr><br><br>
<% end %>
<p></p>
<%= link_to 'Home', root_path %>